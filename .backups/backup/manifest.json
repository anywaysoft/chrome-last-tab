{
  "manifest_version": 3,
  "name": "Tab History Navigator",
  "version": "1.0",
  "description": "Navigates to the previously active tab when a tab is closed",
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
